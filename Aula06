'''
def cauculaIdade(dia, mes, ano):
    if mes == 6 and dia >= 17:
        idade = 2022 - ano
    elif mes == 6 and dia < 17:
    elif mes > 6:
        idade = 2022 - ano - 1
    elif mes < 6:
        idade = 2022 - ano
'''
